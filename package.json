    "typescript": "5.7.3"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.53.1",
    "@tinacms/cli": "^1.9.2",
    "@types/node": "^22.13.9",
    "eslint": "^9.20.1",
    "eslint-config-next": "^15.2.2"
  }
}
